#app
APP_NAME="Schwab to Discord"

#Config
TIME_DELTA_DAYS=3

#Schwab API
SCHWAB_APP_KEY="your_app_key_here"
SCHWAB_APP_SECRET="your_app_secret_here"
CALLBACK_URL="https://127.0.0.1"
TOKENS_DB="/data/tokens.db"
SCHWAB_TIMEOUT=10
ORDER_STATUS="FILLED"

#Discord - Primary webhook
DISCORD_CHANNEL_ID=your_channel_id
DISCORD_WEBHOOK=https://discord.com/api/webhooks/your_webhook_here

#Discord - Secondary webhook (optional, posts to both if set)
DISCORD_WEBHOOK_2=https://discord.com/api/webhooks/your_second_webhook_here

#Data paths
DB_PATH="/data/trades.db"
EXPORT_PATH="/data/trades.xlsx"

#Discord message template for Option Bot
TEMPLATE="**Option Bot**\nTicker: {symbol}\nStrike: {description}\nEntry Price: {price}\nQuantity: {total_sold} sold | {filled_quantity} filled | {position_left} left to sell"
